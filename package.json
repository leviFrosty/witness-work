{
  "name": "witness-work",
  "version": "1.37.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "dev": "APP_VARIANT=development expo start --ios",
    "dev:prod": "expo start --no-dev --minify",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "clean": "rm -rf node_modules .tamagui .expo .cache *.tar.gz ios android",
    "prebuild": "expo prebuild --clean",
    "build:ios": "eas build -p ios --profile development-simulator --local",
    "build:ios-production": "eas build -p ios --profile production-simulator --local --clear-cache",
    "build:android": "eas build -p android --profile development-simulator --local",
    "translate": "bun run ./src/scripts/translate.ts",
    "version:bump": "bun run ./src/scripts/bump-version.js",
    "prepare": "husky",
    "test": "vitest",
    "testFinal": "vitest run",
    "testVitest": "",
    "updateSnapshots": "jest -u --coverage=false",
    "format": "prettier --write . --log-level warn --cache",
    "typecheck": "tsc -p ./tsconfig.json",
    "lint": "eslint . --cache --cache-location .cache/.eslintcache",
    "doctor": "expo-doctor",
    "deps": "madge --ts-config ./tsconfig.json App.tsx -c"
  },
