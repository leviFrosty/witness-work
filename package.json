    "dev": "APP_VARIANT=development expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "git clean -xdf node_modules .tamagui .expo .cache *.tar.gz",
    "build:ios": "eas build -p ios --profile development-simulator --local",
