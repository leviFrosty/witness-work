{
  "cli": {
    "version": ">= 7.3.0",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "development",
        "ANDROID_SDK_ROOT": "/Users/levi/Library/Android/sdk"
      },
      "node": "20.14.0"
    },

    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "node": "20.14.0"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "node": "20.14.0",
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    },
    "production-local": {
      "channel": "production",
      "autoIncrement": true,
      "node": "20.14.0",
      "env": {
        "ANDROID_SDK_ROOT": "/Users/levi/Library/Android/sdk"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production-simulator": {
      "distribution": "internal",
      "channel": "production",
      "ios": {
        "simulator": true,
        "image": "macos-sequoia-15.3-xcode-16.2"
      },
      "node": "20.14.0"
    },
    "e2e-test": {
      "withoutCredentials": true,
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "node": "20.14.0"
    }
  },
  "submit": {
    "production": {}
  }
}
